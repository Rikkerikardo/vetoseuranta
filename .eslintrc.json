{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["prettier", "plugin:react/recommended", "google"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier", "react"],
  "rules": {
    "semi": "off",
    "require-jsdoc": "off",
    "quotes": ["error", "double"],
    "object-curly-spacing": "off",
    "indent": "off",
    "max-len": "off",
    "comma-dangle": "off"
  }
}
